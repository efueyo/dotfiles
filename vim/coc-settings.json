{
  "suggest.noselect": false,
  "diagnostic.errorSign": "âœ˜",
  "diagnostic.warningSign": "!",
  "diagnostic.infoSign": "?",
  "diagnostic.checkCurrentLine": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "json",
    "py",
    "go"
  ],
  "coc.preferences.hoverTarget": "float",
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": [
        "go.mod"
      ],
      "filetypes": [
        "go"
      ]
    }
  },
  "go.goplsOptions": {
    "staticcheck": true
  },
  "yaml.format.enable": true
}
